{
  "version": 2,
  "defaults": {
    "backend": {
      "bucket": "buck",
      "region": "us-west-2"
    },
    "providers": {
      "aws": {
        "account_id": 456,
        "region": "us-west-2",
        "profile": "profile",
        "version": "0.12.0"
      }
    },
    "project": "proj",
    "terraform_version": "0.100.0",
    "owner": "foo@example.com",
    "extra_vars": {
      "foo": "bar1"
    }
  },
  "accounts": {
    "foo": {
      "providers": {
        "aws": {
          "account_id": 123
        }
      }
    },
    "bar": {
      "providers": {
        "aws": {
          "account_id": 456
        }
      }
    }
  },
  "modules": {
    "my_module": {}
  },
  "envs": {
    "staging": {
      "extra_vars": {
        "foo": "bar2"
      },
      "components": {
        "vpc": {
          "module_source": "github.com/terraform-aws-modules/terraform-aws-vpc?ref=v1.30.0",
          "extra_vars": {
            "foo": "bar3"
          }
        },
        "comp1": {},
        "comp2": {},
        "comp_helm_template": {
          "kind": "helm_template",
          "eks": {
            "cluster_name": "k8s"
          }
        }
      }
    },
    "prod": {}
  },
  "plugins": {
    "terraform_providers": {
      "provider": {
        "url": "https://example.com/provider.tar.gz",
        "format": "tar"
      }
    },
    "custom_plugins": {
      "custom": {
        "url": "https://example.com/custom.zip",
        "format": "zip"
      }
    }
  }
}