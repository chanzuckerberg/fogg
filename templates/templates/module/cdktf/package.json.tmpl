{
  "name": "{{ .Name }}",
  "version": "{{ .Version }}",
  "author": "{{ .Author }}",
  {{- if .Publish}}
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  {{- else}}
  "private": true,
  "exports": {
    ".": "./src/index.ts"
  },
  {{- end}}
  "scripts": {
    "get": "cdktf get",
    "ca:login": "pnpm -w run ca:login",
    {{- if .Publish}}
    "build": "if [ -d dist ]; then tsc --build -v; else tsc --build -v --force; fi",
    "watch": "tsc --build --watch",
    {{- end}}
    "lint": "eslint",
    "test": "vitest run --passWithNoTests  --coverage",
    "prettier": "prettier --write .",
    "type-check": "tsc -p ./tsconfig.json --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm -rf coverage"
  },
  "dependencies":{{ .CdktfDependencies | toPrettyJson | indent 2 }},
  "devDependencies":{{ .CdktfDevDependencies | toPrettyJson | indent 2 }},
  "peerDependencies":{{ .CdktfPeerDependencies | toPrettyJson | indent 2 }},
  "//": "Auto-generated by fogg. Do not edit"
}
