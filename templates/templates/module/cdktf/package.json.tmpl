{
  "name": "{{ .Name }}",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "test": "jest --passWithNoTests --updateSnapshot",
    "eslint": "eslint . --ext .ts",
    "prettier": "prettier --write .",
    "synth": "npx ts-node --swc -P ./tsconfig.dev.json src/index.ts"
  },
  "dependencies":{{ .CdktfDependencies | toPrettyJson | indent 2 }},
  "devDependencies":{{ .CdktfDevDependencies | toPrettyJson | indent 2 }},
  "peerDependencies":{{ .CdktfPeerDependencies | toPrettyJson | indent 2 }},
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/(test|src)/**/*(*.)@(spec|test).ts?(x)"
    ],
    "clearMocks": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "clover",
      "cobertura",
      "text"
    ],
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "dist"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "dist"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test-reports"
        }
      ]
    ],
    "transform": {
      "^.+\\.[t]sx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.dev.json"
        }
      ]
    }
  },
  "//": "Auto-generated by fogg. Do not edit"
}
